def nice_string(s):
    vowels = "aeiou"
    vowel_count = 0
    for char in s:
        if char in vowels:
            vowel_count += 1
            if vowel_count >= 3:
                break
    vowel_count = sum(1 for char in s if char in "aeiou")
    letter = False
    for i in range(len(s) - 1):
        if s[i] == s[i + 1]:
            letter = True
    disallowed_strings = ["ab", "cd", "pq", "xy"]
    has_disallowed_string = False
    for substring in disallowed_strings:
        if substring in s:
            has_disallowed_string = True
    if vowel_count >= 3 and letter and not has_disallowed_string:
        return True
    else:
        return False
    

    
with open("problem5/day5.txt", "r") as file:
    lines = file.readlines()
nice_count = 0
for line in lines:
    if nice_string(line.strip()):
        nice_count += 1
print(nice_count)